include mixin.jade
doctype html
html
    head
        meta(charset="utf-8")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title
            block page-title
            | Wantoto Inc.
        block page-stylesheets
            link(href="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css",
                 rel="stylesheet")
        block page-scripts-head
            script(src="//cdnjs.cloudflare.com/ajax/libs/retina.js/1.3.0/retina.min.js", async)
    body
        div#page-wrapper
            header#page-header
                block page-navigation
                    nav.navbar.navbar-wtt.navbar-fixed-top(role="navigation"): div.container
                        div.navbar-header
                            button.navbar-toggle.collapsed(type="button",
                                                           data-toggle="collapse",
                                                           data-target="#main-navbar")
                                span.sr-only Navigation
                                span.icon-bar
                                span.icon-bar
                                span.icon-bar
                            a.navbar-brand(href='/'): img(src="/static/images/logo-sm.png")
                        div#main-navbar.collapse.navbar-collapse
                            ul.nav.navbar-nav.navbar-right
                                +navigation-links()
            section#page-content
                block page-content
            footer#page-footer.container-fluid
                block page-footer
                    div.row: div.col-xs-12
                        ul#footer-nav.list-inline
                            +navigation-links()
                    div.row: div.col-xs-12 Copyright &copy; 2014 Wantoto Inc. All Rights Reserved
        block page-scripts-body
            script(src="//cdnjs.cloudflare.com/ajax/libs/jquery/2.1.1/jquery.min.js")
            script(src="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js")
            script: include ../static/scripts/_load_ga.min.js
